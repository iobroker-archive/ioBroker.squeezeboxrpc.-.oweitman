        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "vis1 edit",
            "url": "http://localhost:8082/vis/edit.html#test",
            "trace": true,
            "skipFiles": ["socket.io.js"]
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "vis2 edit",
