                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "outputplayerdebug": {
                    "type": "checkbox",
                    "label": "sb-outputplayerdebug",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "outputplayersilly": {
                    "type": "checkbox",
                    "label": "sb-outputplayersilly",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                }
            }
        }
    }
}
