{
    "i18n": true,
    "type": "tabs",
    "tabsStyle": {
        "width": "calc(100% - 100px)"
    },
    "items": {
        "maintab": {
            "type": "panel",
            "label": "sb-maintab",
            "newLine": true,
            "items": {
                "server": {
                    "type": "text",
                    "label": "sb-server",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_discover": {
                    "type": "selectSendTo",
                    "label": "sb-discover",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "command": "discoverlms",
                    "jsonData": "{}",
                    "onChange": {
                        "calculateFunc": "data.server = data._discover ? data._discover : data.server // console.log('meintest') //&& data.discover && data.server=data.discover"
                    }
                },
                "port": {
                    "type": "text",
                    "label": "sb-port",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "telnetport": {
                    "type": "text",
                    "label": "sb-telnetport",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "username": {
                    "type": "text",
                    "label": "sb-username",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "password": {
                    "type": "password",
                    "label": "sb-password",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                },
                "TPE2Handling": {
                    "type": "select",
                    "label": "TPE2 Handling",
                    "options": [
                        { "label": "TPE2 as Band", "value": "TPE2Band" },
                        { "label": "TPE2 as Artist of Album", "value": "TPE2Artist" }
                    ],
                    "default": "TPE2Artist",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
                }
            }
        },
        "timertab": {
            "type": "panel",
            "label": "sb-timertab",
            "items": {
                "serverrefresh": {
                    "type": "number",
                    "label": "sb-serverrefresh",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "min": 15,
                    "newLine": true
                },
                "playerrefresh": {
                    "type": "number",
                    "label": "sb-playerrefresh",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "min": 200,
                    "newLine": true
                },
                "favoriterefresh": {
                    "type": "number",
                    "label": "sb-favoriterefresh",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "min": 1,
                    "newLine": true
                },
                "discoveryrefresh": {
                    "type": "number",
                    "label": "sb-discoveryrefresh",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "min": 10,
                    "newLine": true
                }
            }
        },
        "performancetab": {
            "type": "panel",
            "label": "sb-performancetab",
            "items": {
                "useplaylist": {
                    "type": "checkbox",
