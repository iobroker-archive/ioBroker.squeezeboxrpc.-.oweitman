            "type": "panel",
            "label": "sb-debugtab",
            "items": {
                "outputserverdebug": {
                    "type": "checkbox",
                    "label": "sb-outputserverdebug",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4,
                    "newLine": true
